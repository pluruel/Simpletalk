<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/matching_new"
    android:layout_width="300dp"
    android:layout_height="180dp"
    android:layout_centerInParent="true"
    android:background ="#00000000"
    android:layout_gravity="center"
    android:orientation = "vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/warning_popup">

        <Button
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:textSize="20sp"
        android:textColor="#000000"
        android:background="@drawable/button_no"
        android:id="@+id/matching_new_cancle"
        android:layout_alignParentRight="true"
        android:layout_marginRight="45dp"
        android:layout_marginTop="115dp"
        />

    <Button
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:textSize="20sp"
        android:background="@drawable/button_yes"
        android:id="@+id/matching_new_confirm"
        android:layout_marginTop="115dp"
        android:layout_marginStart="45dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="다른 파트너를 찾을까요?"
            android:textColor="#000000"
            android:gravity="center"
            android:textSize="25dp"
            android:layout_marginTop="30dp"/>

    </RelativeLayout>
</RelativeLayout>
